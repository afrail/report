	<queryString language="SQL">
		<![CDATA[select 
	(emp_code_bn(a.code) || ', ' || get_com_lookup_bn_name(c.designation_id)|| '<br><b>' || a.employee_name_bangla) || '</b>' emp_details,
	get_branch_wing_group_bn(c.employee_id) brnch,
	f.name ,
	date(f.achivement_date) datte,
	f.bangla_name ,c.seniority_number 
from	
	hrm_employee_personal_information a,
	hrm_employee_official_information c,
	hrm_achivement_information f
where
	1=1
	and c.employee_id = a.id
	and a.id = f.employee_id
	and A.HRM_TYPE = coalesce( $P{P_HRM_TYPE} , A.HRM_TYPE)
	and c.inactive_date is null
	and date(f.achivement_date) between $P{P_FROM_DATE} and $P{P_TO_DATE}
order by
	get_des_serial(c.designation_id),c.seniority_number]]>
	</queryString>

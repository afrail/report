	<queryString>
		<![CDATA[select id,patient_code,patient_name,rank_name,DOB,name,special_for,patin_age,gender,blood_group,prescription_date,
coalesce(height,'')||'<br>'||coalesce(weight,'')||'<br>'||coalesce(spo2,'')||'<br>'||coalesce(ofc,'')  patint_condition, 
'Patient In Time: '||patient_in_time||' & Out Time: '||to_char(prescription_date,'DD/MM/yyyy HH:MI:SSAM') patient_in_time   ,
coalesce(Pulse,'')||'<br>'|| coalesce(BP,'')||'<br>'||coalesce(Pat_temp,'')||'<br>'||coalesce(rr,'') Illness,prescription_no,token_number token_register_no,
case when length(section_name)>2 then section_name else only_patient_code end only_patient_code,section_name,
only_patient_code only_patient_code_ch,
followup_date,follow_up_visit_duration,follow_up_comment,cheif_complain, disease,advice,disposal_name,
case when complain_count>0 then 'Chief Complain:' end cheif_complain_display,investigation,
case when disease_count>0 then 'Diseases:' end Diseases_display,others_advice,'<b>'||referred_doctor_dis||'</b>'||referd_doctor referd_doctor,referred_doctor_dis,
case when investigation_count>0 then 'Investigation:' end inv_disp,
case when advice_count>0 then 'Advice:' end Advice_disp,
case when inv_finding_count>0 then 'Investigation Finding:' end Inv_finding,
case when illness_history_count>0 then 'Past Illness:' end illness_history_disp,
illness_history,
investigation_finding,
follow_up_visit,followup_disp from(
select eppm.id,epi.patient_code, case when epi.relation_type>0 then trim(epi.patient_name)||chr(10)||
get_com_lookup_name(epi.relation_type)||' of '||get_patient_name_with_rank(epi.patient_id)
 else get_patient_name_with_rank(epi.id) end patient_name ,others_advice, date(epi.date_of_birth) DOB, a.name, a.special_for,
case when epi.date_of_birth>'1930-01-01' then get_age_as_date(date(epi.date_of_birth),date(eppm.prescription_date)) else '' end patin_age, get_com_lookup_name(epi.gender_id) gender ,
get_com_lookup_name(epi.blood_group_id) blood_group, 
eppm.referred_doctor referd_doctor, coalesce(get_com_lookup_name(epi.designation_id),epi.designation_name) rank_name,
eppm.prescription_date prescription_date,
(select to_char(entry_date,'DD/MM/yyyy HH:MI:SS AM') from ehm_token_register etr 
where id=token_register_id) patient_in_time,
(select token_number from ehm_token_register etr 
where id=token_register_id)  token_number,
case when length(eppm.referred_doctor)>0 then 'Referred Doctor/Hospital: ' end referred_doctor_dis,
case when length("temp")>0 then '<b>'||'Temp:'||'</b>'||"temp"||'Â°F,' end Pat_temp,
case when length(height)>0 then '<b>'||' Height: '||'</b>'||height||'",' end height,
case when length(weight)>0 then '<b>'||' Weight: '||'</b>'||weight||'Kg,' end weight,
case when length(ofc)>0 then  '<b>'||' OFC: '||'</b>'||ofc  end ofc, 
case when length(spo2)>0 then '<b>'||' SPO2: '||'</b>'||spo2||'%,' end spo2,
case when length(illness)>0 then '<b>'||'Illness: '||'</b>'||illness||',' end Illness,
case when length(pulse)>0 then '<b>'||' Pulse: '||'</b>'||pulse||'/min,' end Pulse,
case when length(bp)>0 then '<b>'||' BP: '||'</b>'||bp||'mmhg,' end BP,
case when length(rr)>0 then '<b>'||' R/R: '||'</b>'||rr end rr,
eppm.prescription_no,
eppm.disposal_date,eppm.token_register_no, 
translate(epi.patient_code,'0123456789-()!@#$%^&*_','') only_patient_code,
(select get_com_lookup_name(coalesce(coalesce(coalesce(heoi.shop_id,heoi.group_id), heoi.wing_id),heoi.branch_id)) section_name
from hrm_employee_official_information heoi , hrm_employee_personal_information hepi 
where hepi.id=heoi.employee_id 
and hepi.code=epi.emp_code) section_name,
eppm.disposal_duration,
case when disposal_duration=1 and  disposal_dwmy=1 then 'Day' 
when disposal_duration>1 and  disposal_dwmy=1 then 'Days'
when disposal_duration=1 and  disposal_dwmy=7 then 'Week'
when disposal_duration>1 and  disposal_dwmy=7 then 'Weeks'
when disposal_duration=1 and  disposal_dwmy=30 then 'Month'
when disposal_duration>1 and  disposal_dwmy=30 then 'Months' end  disposal_duration_name,
 (eppm.follow_up_visit_duration+1)  followup_date ,
eppm.follow_up_visit_duration,eppm.follow_up_comment ,
	 (select string_agg(cheif_complain,'<br>'  order by id) as cheif_complain from (
	 select id, chief_complaint||' '||coalesce (cast(duration as varchar),'') ||' '||
	 coalesce((case when duration=1 and  dwmy=1 then 'Day'
	 when duration=1 and  dwmy=1 then 'Day'
	 when duration>1 and  dwmy=1 then 'Days'
	 when duration=1 and  dwmy=7 then 'Week'
	 when duration>1 and  dwmy=7 then 'Weeks'
	 when duration=1 and  dwmy=30 then 'Month'
	 when duration>1 and  dwmy=30 then 'Months'
	 when duration=1 and  dwmy=365 then 'Year'
	 	 when duration>1 and  dwmy=365 then 'Years' end),'')  cheif_complain
	 from ehm_pat_pres_chief_complaint
	 where  pat_pres_master_id=$P{prescription_master_id}) ch) cheif_complain,
	 (SELECT  count(*)  cheif_complain_count
FROM ehm_pat_pres_master eppm,
	ehm_pat_pres_chief_complaint eppcc
WHERE 
	 1 = 1 
	 AND eppm.id = eppcc.pat_pres_master_id 
	 AND eppm.id =$P{prescription_master_id}) complain_count,
	 (select  string_agg( get_ehm_com_lookup_name(disease_id)||coalesce(case when length(on_examination)>0
	 then '-'||on_examination end,''),'<br>' order by id) as disease
from ehm_pat_pres_disease
where 1=1
and pat_pres_master_id= $P{prescription_master_id}
group by pat_pres_master_id) disease,
(select  count(*) disease_count
from ehm_pat_pres_disease
where 1=1
and pat_pres_master_id= $P{prescription_master_id}) disease_count,
(select  count(*) inv_finding_count
from ehm_pat_pres_investigation_finding
where 1=1
and pat_pres_master_id= $P{prescription_master_id}) inv_finding_count,
(select string_agg(investigation,'<br>' order by id) as investigation 
from ehm_pat_pres_investigation
where 1=1
and pat_pres_master_id= $P{prescription_master_id}
group by pat_pres_master_id) investigation,
(select string_agg(investigation||' '||finding,'<br>' order by id) as investigation_finding 
from ehm_pat_pres_investigation_finding
where 1=1
and pat_pres_master_id= $P{prescription_master_id}
group by pat_pres_master_id) investigation_finding,
(select string_agg( illness_history,'<br>'  order by id) as cheif_complain from (
 select get_ehm_loopup_name(illness_name_id)||'  '||coalesce (disposal,'')  illness_history,
 epih.id id
 from ehm_patient_illness_history epih , ehm_pat_pres_master m
 where epih.patient_info_id=m.patient_info_id 
 and m.id=$P{prescription_master_id}) eh ) illness_history, 
 (select count(*)  illness_history_count
 from ehm_patient_illness_history epih , ehm_pat_pres_master m
 where epih.patient_info_id=m.patient_info_id 
 and m.id=$P{prescription_master_id}) illness_history_count,
(select count(*) investigation_count 
from ehm_pat_pres_investigation
where 1=1
and pat_pres_master_id= $P{prescription_master_id}) investigation_count,
(select  string_agg(coalesce(get_ehm_com_lookup_name(advice_id),''),'<br>' order by id) as  advice
from ehm_pat_pres_advice 
where 1=1
and pat_pres_master_id= $P{prescription_master_id}
group by pat_pres_master_id) advice,
(select  count(*) advice_count
from ehm_pat_pres_advice 
where 1=1
and pat_pres_master_id= $P{prescription_master_id}) advice_count,
(select string_agg(disposal_name,'<br>'  order by id) as disposal_name from (
 select id, a.disposal_name||' '||
 coalesce((case when a.disposal_duration =1 and  a.disposal_dwmy=1 then 'Day'
 when a.disposal_duration =1 and  a.disposal_dwmy=1 then 'Day'
 when a.disposal_duration>1 and  a.disposal_dwmy=1 then 'Days'
 when a.disposal_duration=1 and  a.disposal_dwmy=7 then 'Week'
 when a.disposal_duration>1 and  a.disposal_dwmy=7 then 'Weeks'
 when a.disposal_duration=1 and  a.disposal_dwmy=30 then 'Month'
 when a.disposal_duration>1 and  a.disposal_dwmy=30 then 'Months'
 when a.disposal_duration=1 and  a.disposal_dwmy=365 then 'Year'
 when a.disposal_duration>1 and  a.disposal_dwmy=365 then 'Years' end),'')||''||to_char(a.disposal_date,'dd-MM-yyyy')  disposal_name
 from ehm_pat_pres_disposal a
 where  pat_pres_master_id=$P{prescription_master_id}) dis) disposal_name,
case when follow_up_visit_duration>0 then 
'Followup:' end followup_disp,
case when follow_up_visit_duration>0 then 
'After '||follow_up_visit_duration||' day/days your followup date '||to_char(date(prescription_date)+follow_up_visit_duration,'dd-mm-yyyy') end follow_up_visit
from ehm_pat_pres_master eppm
left join ehm_patient_info epi on eppm.patient_info_id=epi.id 
left join ehm_doctor_and_staff_information a on eppm.doctor_info_id = a.id
where 1=1
and eppm.id= $P{prescription_master_id}) g]]>

	<queryString>
		<![CDATA[select get_fin_com_lookup_bn_name(fsb.subsidiary_bill_type_id) bill_type,
fsb.bill_number||'/'||get_fin_com_lookup_bn_name(fsb.subsidiary_bill_type_id)||'/বিওএফ/অর্থ' bill_number,
to_char(fsb.bill_date,'dd-MM-yyyy') bill_date,fsb.order_numer order_number_1,bill_source,fsb.reference_number,
case when bill_source=1 then
'দৈনিক আদেশ নামা ২য় খন্ড নম্বর -'||(fsb.order_numer)||' ,তারিখ '||to_char(date(fsb.order_date),'DD')||' '||
 get_eng_month_bangla(cast(to_char(date(fsb.order_date),'MM') as INT))||to_char(date(fsb.order_date),' YYYY')
 when bill_source in(2,3) then fsb.reference_number end 
order_number,fsb.order_date,case when fsb.tax>0 then 'আয়কর  বাদে' else '' end tax_head,
case when fsb.vat>0 then 'ভ্যাট বাদে' else '' end vat_head,
fsb.tax tax_amount , fsb.amount,
fsb.vat vat_amount
,fsb.subject
,fsb.net_amount net_amount,fsb.employee_id,
value_in_bangla(round(fsb.net_amount::numeric,2))  in_word_bangla,coalesce(emp_code_bn(get_emp_code(fsb.employee_id)),'') ||' '||
coalesce(get_emp_bn_name_with_rank (fsb.employee_id),'')||', '||coalesce(get_com_lookup_bn_name(fsb.designation_id),'')||fsb.bill_details||' বিলটি রাজস্ব টিকেট দ্বারা পূর্বস্বীকৃত। বিলটি নিরীক্ষান্তে পাশ করে ব্যাংক হিসাব নম্বর  '||coalesce((select coalesce(bank_account_number,'') ||coalesce(get_bank_branch(bank_branch_id),'')
from hrm_employee_official_information where employee_id=fsb.employee_id),'')||
coalesce((select coalesce(bank_acc,'') 
from budget_bof_supplier where id=fsb.supplier_id),'')||' এর অনুকূলে ইএফটি/চেক প্রদান করার অনুরোধ করা হলো।'||coalesce (case when bill_source in(1,2) then ' এন আইডি- '||coalesce((select coalesce(nid,'') from hrm_employee_personal_information
where id=fsb.employee_id),'')||coalesce((select coalesce(nid,'') from budget_bof_supplier
where id=fsb.supplier_id),'') end,'')||'। এ ব্যয় বাজেট কোড নম্বর '||bec.economic_code||'  হতে বিকলনযোগ্য।'
 economic_code 
from fin_subsidiary_bill fsb, budget_economic_code bec
where 1=1
and fsb.economic_code_id = bec.id 
and fsb.id =  $P{subsidiary_bill_id}]]>

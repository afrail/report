		<parameter name="P_EMPLOYEE_CODE_S" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[SELECT noc_details.NAME ,
CASE WHEN 
	PERSONAL.GENDER_ID=19 AND noc_details.RELATION='Spouse'  
THEN 
	'স্ত্রী' 
ELSE 
	'স্বামী' 
END  RELATION , 
RELATION RELATION_ID, 
to_char(noc_details.dob::date,'DD-MM-YYYY') AS DATE_OF_BIRTH , 
noc_details.NID, noc_details.birth_certificate  birth_Reg, 
(
	SELECT EXTRACT(YEAR FROM AGE(noc_details.dob)) AS AGE
)
FROM hrm_noc_master noc_master, hrm_noc_details noc_details, HRM_EMPLOYEE_PERSONAL_INFORMATION PERSONAL
WHERE noc_details.RELATION='Spouse'
AND noc_master.ID = noc_details.master_id 
AND PERSONAL.ID = noc_master.EMP_ID 
AND PERSONAL.CODE = $P{P_EMPLOYEE_CODE_S}]]>
		</queryString>

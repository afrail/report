	<queryString language="SQL">
		<![CDATA[select b.code ||', '|| get_com_lookup_name(b.EMPLOYEE_RANK) ||', '|| b.EMPLOYEE_NAME applicant_Name,
c.COMMISSION_DATE, d.RETIREMENT_DATE, a.FROM_DATE||', '|| a.TO_DATE ||' ('|| a.TOTAL_DAYS ||')' Duration,

	<queryString language="SQL">
		<![CDATA[select  
	n.reference_number sharok_num, to_char(n.noc_date::date,'DD-MM-YYYY')as noc_apply_date , 
	a.employee_name_bangla emp_name_bn, n.care_of_type, n.care_of, n.noc_for,
	n.passport_type, n.appointed, n.character_defined , 
	A.CODE, GET_COM_LOOKUP_bn_NAME(n.designation) designation_bn, n.present_address , 
	a.nid as nid, to_char(a.retired_date::date,'DD-MM-YYYY') as in_active_date, 
	(
		select 
			count(*) 
		from 
			hrm_noc_details nd 
		where nd.master_id = n.id 
		and   a.id= n.emp_id 
		and   a.code = coalesce($P{P_EMPLOYEE_CODE},a.code) 
	) family_count,
	n.passport_office passport_ofc
from 
	hrm_employee_personal_information a ,
	hrm_noc_master n 
where a.id= n.emp_id 
and a.code = coalesce($P{P_EMPLOYEE_CODE},a.code);]]>
	</queryString>

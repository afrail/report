	<queryString language="SQL">
		<![CDATA[select
	a.code || ', ' || get_branch_wing_group_bn(c.employee_id) emp_details,
	get_com_lookup_bn_name(c.designation_id)|| ', ' || a.employee_name_bangla emp_details2,
	to_char(date(c.joining_date), 'dd/mm/yyyy') niyogdt,
	get_com_lookup_bn_name(a.physically_challenged) typee,c.seniority_number,
	'' remarks
from
	hrm_employee_personal_information a,
	hrm_employee_official_information c
where
	1=1
	and c.employee_id = a.id
	and c.inactive_date is null
	and a.physically_challenged is not null
	and A.HRM_TYPE = coalesce( $P{P_HRM_TYPE} , A.HRM_TYPE)
order by
	get_des_serial(c.designation_id),c.seniority_number ]]>
	</queryString>

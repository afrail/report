	<queryString language="SQL">
		<![CDATA[with  emp_info as(select p.id, o.grade_id, '১। জনাব/বেগম - '||concat(p.employee_name_bangla,' - ',p.code)||' এর প্রত্যাশিত শেষ বেতন প্রত্যয়ন পত্র।' emp_det,
get_employee_desig_bn(o.employee_id) desg , to_char(date(p.date_of_birth),'dd-mm-yyyy')||'খ্রিঃ (এসএসসি সনদ মোতাবেক)' dob ,
to_char(date(o.joining_date),'dd-mm-yyyy') joindt, case when a.is_death is not true then to_char(p.retired_date,'dd-mm-yyyy')||' খ্রিঃ (অপরাহ্ন)' else to_char(a.death_date ,'dd-mm-yyyy')||' (মৃত্যুর তারিখ)' end retired_date  ,
case when a.is_death is not true then to_char(p.lpr_date,'dd-mm-yyyy')||' খ্রিঃ ('||to_char(p.retired_date,'dd-mm-yyyy')||' তারিখ পর্যন্ত বেতন ভাতা পরিশোধ যোগ্য )' else 'প্রযোজ্য নয় ('||to_char(a.death_date ,'dd-mm-yyyy')||'তারিখ পর্যন্ত বেতন ভাতা পরিশোধ করা হয়েছে )' end lpr_dat, '' last_sal, get_grade_slab(o.grade_id)||' ('||get_grad_bn_name(o.grade_id)||')' present_grade,
case when to_char(p.lpr_date, 'dd-MM') >= '07-01'
then  '01-07-' ||cast(to_char(p.lpr_date, 'yyyy') as integer) + 1
else '01-07-' ||to_char(p.lpr_date, 'yyyy') end  next_yr_sal_incrs_dt, o.main_salary,
case when to_char(p.lpr_date, 'dd-MM') <= '07-01'
then '01-07-' ||cast(to_char(p.lpr_date, 'yyyy') as integer) - 1 
else '01-07-' ||to_char(p.lpr_date, 'yyyy') end main_salary_date,
coalesce(b.slab_next,'0') nex_salary_slab, a.death_date, 
coalesce(cast(b.slab_next as integer),0)-coalesce(o.main_salary,0) month_incrment,
case when a.is_death is not true then coalesce(o.main_salary,0) - coalesce(get_next_salary_slab(o.grade_id,o.employee_id),0)||'/-'||' দৈনিক আদেশনামা ২য় খন্ড নম্বর '|| coalesce(a.increment_order_no,'')|| ' তারিখ:'||
coalesce(to_char(a.increment_order_date, 'dd-MM-yyyy'),'') else 'প্রযোজ্য নয়' end increment_do, a.is_death 
from hrm_employee_personal_information p ,hrm_employee_official_information o, fin_ndc_master a,v_hrm_employee b 
where p.id=o.employee_id 
and a.employee_id = p.id
and b.code = $P{P_EMP_CODE}
and p.code= $P{P_EMP_CODE})
select ss.emp_det, ss.desg, ss.dob, ss.joindt, 
ss.retired_date,
ss.lpr_dat, ss.present_grade, 
ss.next_yr_sal_incrs_dt,ss.main_salary,
case when ss.is_death is not true then ss.main_salary_date else to_char(ss.death_date,'dd-MM-yyyy') end main_salary_date,ss.nex_salary_slab,
ss.month_incrment,ss.increment_do, 
case when ss.is_death is true then 'মৃতুর তারিখ : '||to_char(ss.death_date,'dd-MM-yyyy') else 'মূল বেতন- '|| coalesce(ss.nex_salary_slab,'0')||'/- ( তারিখ '||next_yr_sal_incrs_dt||' ) বার্ষিক বর্ধিত বেতনসহ শুধুমাত্র পেনশন ক্ষেত্রে প্রযোজ্য' end prl_increment_salary 
from emp_info ss]]>

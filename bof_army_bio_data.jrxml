	<parameter name="P_PERSONAL_ID" class="java.lang.String"/>
	<parameter name="hrm_bio_part2" class="java.lang.String" isForPrompting="false"/>
	<parameter name="hrm_bio_part3" class="java.lang.String" isForPrompting="false"/>
	<parameter name="hrm_bio_part4" class="java.lang.String" isForPrompting="false"/>
	<parameter name="hrm_bio_part5" class="java.lang.String" isForPrompting="false"/>
	<parameter name="hrm_bio_part6" class="java.lang.String" isForPrompting="false"/>
	<parameter name="hrm_bio_part7" class="java.lang.String" isForPrompting="false"/>
	<queryString language="SQL">
		<![CDATA[select
	a.id employee_id,
	a.code,
	a.employee_name emp_details,
	get_com_lookup_name(a.employee_rank) rank,
	 (
	select
		commission_type || ', ' || to_char(Date(c.commission_date) , 'dd/MM/YYYY')
	from
		HRM_OFFICIAL_COMMISION_INFORMATION c
	where
		c.employee_id = a.id
	order by
		id asc
	limit 1) dt_com,
	to_char(Date(a.date_of_birth) , 'dd/MM/YYYY')|| ', ' || get_com_lookup_name(a.permanent_district) DOB,
	get_com_lookup_name(a.regiment_id) arm_svc,
	('Vill- ' || a.permanent_village_house) per_village,
	('P.O.- ' || a.permanent_post_office) per_post_office,
	('P.S.- ' || get_com_lookup_name(a.permanent_upazila) ) per_police_station,
	('Dist.- ' || get_com_lookup_name(a.permanent_district) ) per_district ,
	get_com_lookup_name(a.marital_status_id) || case
		to_char(date(a.marrige_date), 'dd/mm/yyyy') when null then to_char(date(a.marrige_date), 'dd/mm/yyyy')
		else ', ' || to_char(Date(a.marrige_date) , 'dd/MM/YYYY')
	end marital_status,
		(
	select
		COUNT(f.id) son_count
	from
		hrm_employee_family_information f
	where
		a.id = f.employee_id
		and f.relation = '42' ) son_count,
		(
	select
		COUNT(f.id) daughter_count
	from
		hrm_employee_family_information f
	where
		a.id = f.employee_id
		and f.relation = '43' ) daughter_count ,
	'' armed_force_relative, 
	(
	select 
	coalesce (			
	string_agg(
				case
					when (select count(*)
						  from
							hrm_employee_personal_information a,	
							hrm_employee_official_information c, 
							hrm_visit_information v
						  where 1 = 1 
						  	and c.employee_id = a.id
							and a.id = v.employee_id
							and c.is_civil = false
							and a.code = $P{P_PERSONAL_ID}) > 0 then get_com_lookup_name(v.country_id)
				end, ', '
				), 'N/A')
	from 
				 hrm_employee_official_information b, 
		 		 hrm_visit_information v
	where
		1 = 1
		and b.employee_id = a.id
		and a.id = v.employee_id
		and b.is_civil = false
	) country_visit
from
	hrm_employee_personal_information a
left join hrm_employee_official_information b on
	b.employee_id = a.id
where
	1 = 1
	and a.employee_rank is not null
	and a.hrm_type = 1
	and b.inactive_date is null
	and a.code = $P{P_PERSONAL_ID}]]>
	</queryString>

	<parameter name="P_APP_ID" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select 
	m.gate_pass_type_id, get_gate_pass_type(m.gate_pass_type_id) gp_type, 
	CASE WHEN m.gate_pass_type_id=4 THEN 'y' ELSE 'n' END  is_ms, d.spouse_name visitor_spouse, 
	d.visitor_name as name, to_char(d.visitior_date_of_birth::date,'DD-MM-YYYY') dob, 
	EXTRACT(YEAR FROM AGE(d.visitior_date_of_birth)) visitor_age,  
	CASE WHEN EXTRACT(YEAR FROM AGE(d.visitior_date_of_birth)) > 40 THEN 'y' ELSE 'n' END  is_40_plus, 
	d.father_name, d.mother_name, 'অস্থায়ী' appoint_type, 
	d.present_address , d.permanent_address , d.mobile_no, GET_COM_LOOKUP_BN_NAME(d.religion_id) religion, 
	GET_COM_LOOKUP_BN_NAME(d.marital_status_id) marital_status, get_gate_bn_name(m.used_gate_id) used_gate, 
	m.gate_pass_purpose app_purpose , e.employee_name_bangla applicant_name, 
	GET_COM_LOOKUP_BN_NAME(m.reference_employee_designation_id) applicant_desig,  
	d.nid_or_birth_certificate_no, d.photo_url , 
	(
		select 
			CASE WHEN e.GENDER_ID=19 THEN 'স্ত্রীর' ELSE 'স্বামীর' END  RELATION  
		from 
			HRM_EMPLOYEE_FAMILY_INFORMATION f
		where 
	 		f.RELATION=41 
		and	e.ID = f.EMPLOYEE_ID
	) applicant_spouse_label, m.applicants_spouse_designation  spouse_desig,  m.applicants_spouse_workplace  spouse_ws,
	GET_COM_LOOKUP_BN_NAME(e.marital_status_id) app_marital_status, 
	to_char(m.entry_date::date,'DD-MM-YYYY') app_date, GET_COM_LOOKUP_BN_NAME(o.wing_id) applicant_section  ,  
	e.code applicant_personal_code, e.present_village_house house_no, e.mobile_number  applicant_mobile 
from 
	clr_gate_pass_application_master m, 
	clr_gate_pass_application_detail d, 
	hrm_employee_personal_information e,
	hrm_employee_official_information o  
where 
	o.employee_id  = e.id 
and m.reference_employee_id  = e.id 
and d.application_master_id  = m.id
and m.id = $P{P_APP_ID}  -- '2376']]>
	</queryString>

	<parameter name="P_FINANCIAL_YEAR" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[with 
current_demand as 
	( 
		select 
			d.economic_code_id,
			sum(coalesce(d.approved_mod_amount,	0.0)) current_budget
		from budget_demand_details d 
		join budget_financail_year fy on fy.id = d.financial_year_id
		where 1=1 
		and fy.financial_year = $P{P_FINANCIAL_YEAR} 
		and d.budget_type_id = 3 
		group by d.economic_code_id  --d.budget_type_id = 3
	),
current_halnagad as 
	(
	select 
			d.economic_code_id,
			sum(coalesce(d.bill_amount ,	0.0)) halnagad
		from budget_bill_expenditure d 
		join budget_financail_year fy on fy.id = d.financial_year_id
		where 1=1 
		and fy.financial_year = $P{P_FINANCIAL_YEAR} 
		--and d.budget_type_id = 3 
		group by d.economic_code_id		
	)
select
	$P{P_FINANCIAL_YEAR} financial_year,
	id, name, name_bangla, economic_code,
	parent_name, parent_name_bangla, parent_economic_code, 
	coalesce(get_amount_in_thousand(current_demand.current_budget), 0.0) current_budget, 
	coalesce(get_amount_in_thousand(current_halnagad.halnagad), 0.0) halnagad,
	coalesce(get_amount_in_thousand(current_demand.current_budget - current_halnagad.halnagad), 0.0) remainn, 
	coalesce((current_halnagad.halnagad * 100) / current_demand.current_budget, 0.0) per, 
	'অর্থ' brnch
from v_budget_eco_code_selfjoined v_ecode 
left join current_demand 	on current_demand.economic_code_id		= v_ecode.id
left join current_halnagad 	on current_halnagad.economic_code_id	= v_ecode.id
order by parent_economic_code,
		 economic_code]]>
	</queryString>

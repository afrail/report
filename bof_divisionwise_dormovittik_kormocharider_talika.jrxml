	<queryString language="SQL">
		<![CDATA[select
	emp_code_bn(a.code)|| ', ' || get_com_lookup_bn_name(c.designation_id)|| ', ' || a.employee_name_bangla emp_details,
	get_branch_wing_group_bn(c.employee_id) brnch,
	get_com_lookup_bn_name(a.religion_id) religion
from
	hrm_employee_personal_information a,
	hrm_employee_official_information c
where
	1=1
	and a.id = c.employee_id 
	and c.inactive_date is null
	and c.employee_type_id = 36
	and a.HRM_TYPE = coalesce( $P{P_HRM_TYPE} , a.HRM_TYPE)
	and a.religion_id = $P{P_RELIGION_ID}-- 9 = Islam, 10 = Hindu, 832 = Christian, 833 = Buddha
	and c.branch_id = coalesce ($P{P_BRANCH_ID}, c.branch_id)
	and c.wing_id = coalesce ($P{P_WING_ID}, c.wing_id) or null 
	and c.group_id = coalesce ($P{P_GROUP_ID}, c.group_id) or null 
	and c.shop_id = coalesce ($P{P_SHOP_ID}, c.shop_id) or null 
	and c.line_id = coalesce ($P{P_LINE_ID}, c.line_id)	or null 
order by
	a.religion_id,
	get_des_serial(c.designation_id),
	c.branch_id]]>
	</queryString>

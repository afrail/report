	<queryString language="SQL">
		<![CDATA[select hepi.code, hepi.employee_name_bangla,
get_com_lookup_bn_name(heoi.designation_id) designation_name ,
get_com_lookup_bn_name(heoi.billing_office) section_name, 
case when $P{P_FROM_MONTH} =$P{P_TO_MONTH} then get_eng_month_bangla($P{P_FROM_MONTH}  )||' '||$P{P_FROM_YEAR} 
else
get_eng_month_bangla( $P{P_FROM_MONTH}  )||' '||$P{P_FROM_YEAR} ||' হতে '|| get_eng_month_bangla( $P{P_TO_MONTH}  )||' '||$P{P_TO_YEAR}|| ' পর্যন্ত' end month_bn, 
 '' month_bn2,
sum(basic_salary) basic_salary,
sum(house_rent_allowance) house_rent_allowance,
sum(medical_allowance)medical_allowance,
sum(tiffin_allowance)tiffin_allowance ,
sum(conveyance_allowance) conveyance_allowance,
sum(education_allowance)education_allowance ,
sum(cleaning_allowance) cleaning_allowance,
sum(danger_allowance)danger_allowance,
sum(entertainment_allowance)entertainment_allowance,
sum(others_allowance)others_allowance,-- praptto 
sum(bf)bf ,
sum(future_fund)future_fund,
sum(gpf_1st)gpf_1st ,
sum(gpf_2nd) gpf_2nd,
sum(gpf_3rd) gpf_3rd,
sum(house_building_or_repairing_capital) house_building_or_repairing_capital,
sum(house_building_or_repairing_interest) house_building_or_repairing_interest,
sum(motorcycle) motorcycle,
sum(motorcycle_interest) motorcycle_interest,
sum(house_rent_deduction)house_rent_deduction,
sum(gas)gas ,
sum(water)water ,
sum(electricity)electricity ,
sum(others_deduction) others_deduction,
COALESCE(sum(income_tax),0)income_tax ,
sum(stamp_duty)stamp_duty ,
SUM(COALESCE(total,0) ) total,
sum(total_deduction ) total_deduction,
sum(nit_payable) nit_payable, 
cast((select COALESCE(sum(total_amount),0) from bof_over_time_rpt   where emp_code=hepi.code ) as numeric) overtime,
(select COALESCE(sum(amount),0) from fin_subsidiary_bill fsb where subsidiary_bill_type_id=54) rection,
(select COALESCE(sum(bonus_amount),0) from fin_employee_bnous   where employee_code=hepi.code ) bonus, 
(select COALESCE(sum(bonus_amount),0) from fin_employee_bnous   where employee_code=hepi.code and bonus_type=66) boshakhi_bnous
from fin_monthly_salary_process fmsp , hrm_employee_personal_information hepi ,
hrm_employee_official_information heoi 
where 1=1
and hepi.id=fmsp.employee_personal_information_id 
and heoi.employee_id=hepi.id
and  (  $P{P_EMP_CODE}  is null or hepi.code=  $P{P_EMP_CODE} )
and "year"||"month" between $P{P_FROM_YEAR}||$P{P_FROM_MONTH} and   $P{P_TO_YEAR}||$P{P_TO_MONTH}  
group by  hepi.code, hepi.employee_name_bangla,
heoi.designation_id ,
heoi.billing_office]]>

	<parameter name="bof_common_header_A4_portrait_bangla" class="java.lang.String" isForPrompting="false"/>
	<parameter name="bof_common_footer_A4_portrait_bangla" class="java.lang.String" isForPrompting="false"/>
	<parameter name="P_FROM_MONTH" class="java.lang.Integer"/>
	<parameter name="P_TO_MONTH" class="java.lang.Integer"/>
	<parameter name="P_SIGNATURE_AUTHORITY" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[select
	(select 
		count(o.id)
	 from 
	 	hrm_employee_official_information o,
	 	hrm_employee_personal_information p
	 where 1=1
	 and o.employee_id = p.id 
	 and p.hrm_type = 1
	 and p.active = true 
	 and o.is_civil = false
	 and o.inactive_date is null
	) army_count,
	sum(cnt) cnt,
	sum(d_15) d_15,
	sum(d_21) d_21,
	sum(d_30) d_30 ,
	sum(d_15) + sum(d_21) + sum(d_30) ttt,
	sum(cnt) - (sum(d_15) + sum(d_21) + sum(d_30)) rem_lev,
--	(sum(d_15) + sum(d_21) + sum(d_30)/sum(cnt)) * 100 perct,
	case
		when sum(cnt) = 0 then 0
		else
		(sum(d_15) + sum(d_21) + sum(d_30)/sum(cnt)) * 100
	end
	perct,
	'' remarks,
	get_emp_bn_name_with_rank($P{P_SIGNATURE_AUTHORITY})|| chr(10)|| get_employee_desig_bn($P{P_SIGNATURE_AUTHORITY})||
chr(10)|| 'পক্ষে কমান্ডেন্ট' p_authority ,
--	to_char(current_date, 'Month, YYYY') tt,
	get_eng_month_bangla(cast(to_char(current_date, 'MM') as Integer))||',' || to_char(current_date, 'YYYY') tt,
	get_eng_month_bangla($P{P_FROM_MONTH})||'-' ||get_eng_month_bangla($P{P_TO_MONTH})||' ' || to_char(current_date, 'YYYY') monthDuration
from
	(
	select
		count(a.id) cnt,
		0 d_15,
		0 d_21,
		0 d_30
	from
		hrm_employee_personal_information a,
		hrm_employee_official_information c,
		hrm_employee_leave_information l
	where
		1 = 1
		and c.employee_id = a.id
		and a.id = l.employee_id
		and c.is_civil = false
		and l.leave_type_id = 51
		--anual lev	
union all
	select
		0 cnt,
		case
			when l.total_days = 15 then count(*)
			else 0
		end d_15,
		case
			when l.total_days >15
			and l.total_days <= 21 then count(*)
			else 0
		end d_21,
		case
			when l.total_days > 21
			and l.total_days <= 30 then count(*)
			else 0
		end d_30
	from
		hrm_employee_personal_information a,
		hrm_employee_official_information c,
		hrm_employee_leave_information l
	where
		1 = 1
		and c.employee_id = a.id
		and a.id = l.employee_id
		and c.is_civil = false
		and l.leave_type_id = 51
		and cast(to_char(l.from_date , 'MM') as Integer) >= $P{P_FROM_MONTH}
		and cast(to_char(l.to_date , 'MM') as Integer) <= $P{P_TO_MONTH}
		--anual lev
	group by
		l.total_days			
			) x]]>
	</queryString>

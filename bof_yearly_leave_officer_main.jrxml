		<parameter name="P_FROM_MONTH" class="java.lang.Integer"/>
		<parameter name="P_TO_MONTH" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[select
a.code || ' ' || coalesce(get_com_lookup_bn_name(a.EMPLOYEE_RANK) || ' ' || a.employee_name_bangla, a.employee_name_bangla) || ' , ' || get_com_lookup_bn_name(c.designation_id)  emp_details,
	get_com_lookup_bn_name(c.designation_id) designation,
	'বিওএফ' sontha,
	to_char(date(l.from_date), 'dd/mm/yyyy') from_date,
	to_char(date(l.to_date), 'dd/mm/yyyy') to_date,
	l.total_days,
	'' remarks
from	
	hrm_employee_personal_information a,
	hrm_employee_official_information c,
	hrm_employee_leave_information l
where
	1 = 1
	and a.id = c.employee_id
	and a.id = l.employee_id
	and c.is_civil = false
	and l.leave_type_id = 51
	and cast(to_char(l.from_date , 'MM') as Integer) >= $P{P_FROM_MONTH}
	and cast(to_char(l.to_date , 'MM') as Integer) <= $P{P_TO_MONTH}
	--anual lev
order by
	get_des_serial(c.designation_id)]]>
		</queryString>

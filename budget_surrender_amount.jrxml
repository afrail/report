	<queryString language="SQL">
		<![CDATA[with budget_surrender_detail as (
select economic_code_id codeid, basic_budget , re_basic_budget , re_projection_budget , surrendered_amount , expense_amount , "comments"  
from budget_surrender_details b
where 1=1 
		and b.financial_year_id = $P{P_FINANCIAL_YEAR_ID} 
)	
select (select bfy.financial_year from budget_financail_year bfy  where id = $P{P_FINANCIAL_YEAR_ID}) financial_year,id, name, name_bangla, economic_code,
	parent_name, parent_name_bangla, parent_economic_code,
	codeid, basic_budget , re_basic_budget , re_projection_budget , 
	surrendered_amount , expense_amount , "comments" from v_budget_eco_code_selfjoined v_ecode
left join budget_surrender_detail bsd on bsd.codeid = v_ecode.id
order by parent_economic_code, economic_code]]>

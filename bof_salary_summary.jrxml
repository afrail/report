	<queryString language="SQL">
		<![CDATA[select count(id)
 office_total,
get_eng_month_bangla(case when cast($P{P_MONTH} as integer) = 1 then 12 else (cast($P{P_MONTH} as integer)-1) end)||'/ '||(case when cast($P{P_MONTH} as integer) =1 then cast($P{P_YEAR} as integer)-1 else cast($P{P_YEAR} as integer) end) 
||' মাসের সামারী '||'( পরিশোধিত মাস '||get_eng_month_bangla( cast($P{P_MONTH} as integer) ) 
|| '/ '|| $P{P_YEAR}   || ' ) '||
case when  $P{P_HRM_TYPE_ID}=1 then ' - অফিসার'
when $P{P_HRM_TYPE_ID}=2 then '- স্টাফ ' 
when $P{P_HRM_TYPE_ID}=3 then '- কারিগর'
else '' end monthh,
sum(basic_salary) basic_salary,
sum(house_rent_allowance)house_rent_allowance,
SUM(medical_allowance)medical_allowance,
SUM(education_allowance)education_allowance,
SUM(danger_allowance) danger_allowance
,SUM(tiffin_allowance) tiffin_allowance,
SUM(conveyance_allowance) conveyance_allowance
, SUM(others_allowance) others_allowance,
SUM(total) total, office_id, get_com_lookup_bn_name(office_id) Office_name, 
hrm_type,"month",year, 
(select coalesce(sum(total_amount),0) 
from bof_over_time_rpt botr  
where 1=1 and cast (to_char(to_date,'mm')  as integer) = cast($P{P_MONTH} as integer)
and to_char(to_date,'yyyy')  =   $P{P_YEAR} and botr.office_id=fins.office_id ) overt, 
(
select coalesce(sum(amount),0)
from fin_subsidiary_bill fsb , hrm_employee_official_information heoi 
where economic_code_id  in (27)
and heoi.employee_id=fsb.employee_id
and heoi.billing_office=fins.office_id
and cast (to_char(bill_date,'mm')  as integer) = cast($P{P_MONTH} as integer)
and to_char(bill_date,'yyyy')  =   $P{P_YEAR} 
) ent,
(
select coalesce(sum(amount),0)
from fin_subsidiary_bill fsb , hrm_employee_official_information heoi 
where economic_code_id  in (30)
and heoi.employee_id=fsb.employee_id
and heoi.billing_office=fins.office_id
and cast (to_char(bill_date,'mm')  as integer) = cast($P{P_MONTH} as integer)
and to_char(bill_date,'yyyy')  =   $P{P_YEAR} 
) nobob,
(
select coalesce(sum(amount),0)
from fin_subsidiary_bill fsb , hrm_employee_official_information heoi 
where economic_code_id  in (25)
and heoi.employee_id=fsb.employee_id
and heoi.billing_office=fins.office_id
and cast (to_char(bill_date,'mm')  as integer) = cast($P{P_MONTH} as integer)
and to_char(bill_date,'yyyy')  =   $P{P_YEAR} 
) utsob
from fin_monthly_salary_process fins
where  ( $P{P_BILLING_OFFICE} is null or  office_id=  $P{P_BILLING_OFFICE}  ) --1656
 and  (  $P{P_HRM_TYPE_ID} is null or hrm_type = $P{P_HRM_TYPE_ID} )
and cast (month as integer) = cast($P{P_MONTH} as integer)
and year =   $P{P_YEAR} 
group by office_id,hrm_type,month,year]]>
	</queryString>

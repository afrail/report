	<queryString language="SQL">
		<![CDATA[SELECT HEPI.CODE ||' /'|| HEPI.EMPLOYEE_NAME_BANGLA EMP_NAME,
GET_EMPLOYEE_DESIG_BN(HEPI.ID) DESIGNATION,
GET_FIN_COM_LOOKUP_BN_NAME(LOAN_TYPE_ID) LOAN_TYPE,REFERENCE_NO,
LOAN_TYPE_ID, APPLICATION_AMOUNT, APPROVE_AMOUNT,TO_CHAR(LOAN_DATE,'DD/MM/YYYY')LOAN_DATE,MAX(TO_CHAR(LOAN_DATE,'DD/MM/YYYY'))MAX_LOAN_DATE,
TO_CHAR(DISBURSEMENT_DATE,'DD/MM/YYYY')DISBURSEMENT_DATE,INTEREST_AMOUNT,INTEREST_INSTALMEN,INTEREST_RATE,
PRINCIPAL_INSTALMENT,TO_CHAR(REPAYMENT_START_DATE,'DD/MM/YYYY')REPAYMENT_START_DATE, 
(SELECT Count(AMOUNT) FROM FIN_LOAN_ENTRY_DETAILS
WHERE MASTER_ID=1 AND INSTALMENT_TYPE_ID=1 AND PAID=TRUE) LOAN_INS_PAID, 
(SELECT SUM(AMOUNT) FROM FIN_LOAN_ENTRY_DETAILS
WHERE MASTER_ID=1 AND INSTALMENT_TYPE_ID=1 AND PAID=TRUE) LOAN_PAID, 
(SELECT SUM(AMOUNT) FROM FIN_LOAN_ENTRY_DETAILS
WHERE MASTER_ID=1 AND INSTALMENT_TYPE_ID=1 AND PAID=FALSE) LOAN_AMOUNT_UNPAID,
(SELECT count(AMOUNT) FROM FIN_LOAN_ENTRY_DETAILS
WHERE MASTER_ID=1 AND INSTALMENT_TYPE_ID=2 AND PAID=TRUE) INTEREST_INS_PAID, 
(SELECT SUM(AMOUNT) FROM FIN_LOAN_ENTRY_DETAILS
WHERE MASTER_ID=1 AND INSTALMENT_TYPE_ID=2 AND PAID=TRUE) INTEREST_PAID, 
(SELECT SUM(AMOUNT) FROM FIN_LOAN_ENTRY_DETAILS
WHERE MASTER_ID=1 AND INSTALMENT_TYPE_ID=2 AND PAID=FALSE) INTEREST_AMOUNT_UNPAID
FROM FIN_LOAN_ENTRY_MASTER FLEM , HRM_EMPLOYEE_PERSONAL_INFORMATION HEPI 
WHERE 1=1
AND FLEM.EMPLOYEE_ID=HEPI.ID
AND PAGE_TYPE=1
AND HEPI.CODE=$P{P_EMPLOYEE_CODE}
group by HEPI.CODE,HEPI.EMPLOYEE_NAME_BANGLA,HEPI.ID,LOAN_TYPE_ID,REFERENCE_NO
,APPLICATION_AMOUNT, APPROVE_AMOUNT,LOAN_DATE,DISBURSEMENT_DATE,INTEREST_AMOUNT,INTEREST_INSTALMEN,INTEREST_RATE,
